    }
  },
  "flex-layout.col#wishlist":{
    "children": [
      "rich-text#wishlist"
    ],
    "props":{
      "verticalAlign":"middle"
    }
  },

  "rich-text#wishlist": {
    "props": {
      "text": "[wishlist](/wishlist)"
    }
  },
  "image#arrow-right": {
    "props": {
      "src": "https://i.imgur.com/r1VYUKc.png",
      "maxHeight": 24
    }
  }
