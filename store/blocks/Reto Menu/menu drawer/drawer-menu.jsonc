{
  "drawer#menu": {
    "children": [
      "drawer#item-1",
      "menu-item#departamento-2",
      "menu-item#departamento-3",
      "menu-item#departamento-4",
      "menu-item#departamento-5",
      "menu-item#departamento-6",
      "menu-item#departamento-7",
      "flex-layout.row#account-wishlist-container"
    ]
  },
  "flex-layout.row#account-wishlist-container": {
    "children": [
      "flex-layout.col#account",
      "flex-layout.col#wishlist"
    ],
    "props":{
      "preserveLayoutOnMobile":true,
      "colGap":"auto",
      "fullWidth":true,
      "horizontalAlign":"center",
      "verticalAlign":"middle"
    }
  },
  "flex-layout.col#account": {
    "children": [
      "login"
    ],
    "props":{
